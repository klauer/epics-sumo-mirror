^test/data/src/stamp
^test/data/src/base
^test/data/src/support
^test/data/repos/stamp
^test/data/repos/base
^test/data/repos/support
^test/data/epics/stamp
^test/data/epics/base
^test/data/epics/support
^test/tmp-.*
^test/.*\.coverage
^test/htmlcov
^test/.*\.tmp
^test/.*\.bak
^test/.*\.out
^test/.*\.tst

# samples directory:
^samples/IDCP.DB
^samples/R3-14-12-supports.SCAN
^samples/R3-14-12.DB
^samples/R3-14-8-supports.SCAN
^samples/R3-14-8.DB

