# vim swap-files:
.*\.swp
.*\.swm
.*\.swn
.*\.swo

# python:
.*\.pyc

# python build system
^MANIFEST$
^build$

# generated coverage data:
^coverage

# python modules containing documentation, these are generated by
# rst-extract.py:
^python2/sumolib/configuration_doc.py
^python2/sumolib/sumo_doc.py
^python3/sumolib/configuration_doc.py
^python3/sumolib/sumo_doc.py

# newtest directories:
^test/data/src/stamp
^test/data/src/base
^test/data/src/support
^test/data/repos/stamp
^test/data/repos/base
^test/data/repos/support
^test/data/epics/stamp
^test/data/epics/base
^test/data/epics/support
^test/tmp-.*
^test/.*\.tmp
^test/.*\.bak
^test/.*\.out
^test/.*\.tst

# samples directory:
^samples/IDCP.DB
^samples/R3-14-12-supports.SCAN
^samples/R3-14-12.DB
^samples/R3-14-8-supports.SCAN
^samples/R3-14-8.DB

# doc directories:
^doc/_build$

# generated files in doc directory
^doc/.*\.png
^doc/.*\.dia~

# distribution directory:
^dist/
^dist-bitbucket

# local pylint
^pylint-2
