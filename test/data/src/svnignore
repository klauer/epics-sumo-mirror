# vim swap-files:
svn propset svn:ignore "*.swp" .

# python compile files
svn propset svn:ignore "*.pyc" .

# cscope files:
svn propset svn:ignore "/cscope.*" .

# directories to ignore
svn propset svn:ignore "/bin" .
svn propset svn:ignore "/db" .
svn propset svn:ignore "/dbd" .
svn propset svn:ignore "/dl" .
svn propset svn:ignore "/include" .
svn propset svn:ignore "/lib" .

# directories generated by epics that
# are to ignore:
svn propset svn:ignore "O.*" .

